<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox fx:id="threadLayout" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="istvangergo.javaeloadas.Controller.ThreadWindow">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="secondText"/>
    <Label fx:id="thirdText"/>
    <Button fx:id="firstButton" text="Click ME!" onAction="#onFirstButtonClick"/>
    <Button fx:id="secondButton"  text="Click ME too!" onAction="#onSecondButtonClick"/>
</VBox>
